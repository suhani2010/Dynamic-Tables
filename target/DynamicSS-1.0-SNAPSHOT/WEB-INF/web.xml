<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.table.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.table.servlets.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>com.table.servlets.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateTableServlet</servlet-name>
        <servlet-class>com.table.servlets.CreateTableServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertDataServlet</servlet-name>
        <servlet-class>com.table.servlets.InsertDataServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowFilterServlet</servlet-name>
        <servlet-class>com.table.servlets.ShowFilterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteDataServlet</servlet-name>
        <servlet-class>com.table.servlets.DeleteDataServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateTableServlet</servlet-name>
        <url-pattern>/CreateTableServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertDataServlet</servlet-name>
        <url-pattern>/InsertDataServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShowFilterServlet</servlet-name>
        <url-pattern>/ShowFilterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteDataServlet</servlet-name>
        <url-pattern>/DeleteDataServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>login_page.jsp</welcome-file>
    </welcome-file-list>
</web-app>
